{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "funkmap-front"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "web.config"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "assets/font-awesome-4.7.0/css/font-awesome.min.css",
        "assets/icons/icons.css",
        "assets/styles/leaflet.scss",
        "assets/styles/forms.scss",
        "assets/styles/multi-select.scss",
        "assets/styles/tooltip.scss",
        "assets/styles/cropper.scss",
        "assets/styles/modal.scss",
        "assets/styles/slider.scss",
        "../node_modules/leaflet/dist/leaflet.css",
        "../node_modules/cropper/dist/cropper.css",
        "../node_modules/leaflet.markercluster/dist/MarkerCluster.css",
        "../node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.css"
      ],
      "scripts": [
        "../node_modules/leaflet/dist/leaflet.js",
        "../node_modules/leaflet.markercluster/dist/leaflet.markercluster.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/signalr/jquery.signalR.js",
        "../node_modules/cropper/dist/cropper.js",
        "../node_modules/@vimeo/player/dist/player.js",
        "../node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js",
        "../node_modules/leaflet.gridlayer.googlemutant/Leaflet.GoogleMutant.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "serve": {
      "sslKey": "./Bandmap.key",
      "sslCert": "./Bandmap.cer"
    }
  }
}